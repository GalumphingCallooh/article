{
  "name": "@iooxa/ink-article",
  "version": "0.2.0",
  "description": "Scientific web components for interactive scientific writing, reactive documents and explorable explanations.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "explorable explanations",
    "web components",
    "writing"
  ],
  "author": "rowanc1",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**.ts -c .eslintrc.json",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "clean": "rm -rf dist || true; rm -f styles/index.css*;",
    "watch-css": "sass --watch styles/index.scss styles/index.css",
    "build-css": "mkdir -p dist; sass styles/index.scss styles/index.css; cp styles/index.css dist/ink.css",
    "build-dev": "webpack --config webpack.dev.js",
    "build": "npm run build-css; webpack --config webpack.prod.js; tsc; rm -rf dist/src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iooxa/ink.git"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^25.1.4",
    "@types/katex": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.4",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@iooxa/runtime": "^0.2.1",
    "@iooxa/ink-basic": "^0.2.1",
    "highlight.js": "^9.18.1",
    "katex": "^0.11.1",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "bugs": {
    "url": "https://github.com/iooxa/ink/issues"
  },
  "homepage": "https://components.ink",
  "sideEffects": false
}
